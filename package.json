{
  "name": "@munchii/coffeeshop",
  "version": "1.0.0",
  "description": "Website for a fictitious coffee shop.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munchii/coffeeshop.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npm run build-compile && npm run build-resources",
    "build-compile": "tsc",
    "build-resources": "cdk synthesize --app dist/website.js --output dist/cdk.out",
    "deploy": "npm run build && AWS_REGION=us-west-2 AWS_PROFILE=munchii npm run deploy-resources && AWS_REGION=us-west-2 AWS_PROFILE=munchii npm run deploy-content",
    "deploy-resources": "cdk deploy --app dist/website.js --output dist/cdk.out",
    "deploy-content": "aws s3 sync --delete ./public s3://`node dist/get-physical-id.js`",
    "start": "node dist/local-server.js"
  },
  "devDependencies": {
    "@aws-cdk/aws-cloudfront": "1.18.0",
    "@aws-cdk/aws-iam": "1.18.0",
    "@aws-cdk/aws-s3": "1.18.0",
    "@aws-cdk/core": "1.18.0",
    "express": "4.17.1",
    "@types/node": "12.12.7",
    "aws-cdk": "1.18.0",
    "aws-sdk": "2.579.0",
    "typescript": "3.7.2"
  }
}
